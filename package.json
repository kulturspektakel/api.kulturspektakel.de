{
  "name": "api.kulturspektakel.de",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "node -r dotenv/config dist/index.js",
    "build": "tsc",
    "dev": "ts-node-dev --transpile-only --no-notify -r dotenv/config src/index.ts",
    "generate:prisma": "prisma generate",
    "generate:mail": "maizzle build production",
    "generate:proto": "cd src/proto && protoc --experimental_allow_proto3_optional --plugin=../../node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. ./*.proto --ts_proto_opt=esModuleInterop=true",
    "generate:env": "yarn dotenv-vault pull",
    "update:db": "export $(cat .env | xargs) && prisma db push",
    "mail": "maizzle serve"
  },
  "devDependencies": {
    "@maizzle/framework": "4.3.1",
    "@sentry/cli": "^2.11.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/date-fns": "^2.6.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash.merge": "^4.6.7",
    "@types/node-emoji": "^1.8.2",
    "@types/node-fetch": "2.6.2",
    "@types/nodemailer": "^6.4.7",
    "prettier": "^2.8.1",
    "prisma": "^4.8.0",
    "tailwindcss-box-shadow": "^2.0.0",
    "tailwindcss-email-variants": "^2.0.0",
    "tailwindcss-mso": "^1.3.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@awaitjs/express": "^0.9.0",
    "@aws-sdk/client-ses": "^3.236.0",
    "@pothos/core": "^3.23.2",
    "@pothos/plugin-prisma": "^3.38.1",
    "@pothos/plugin-relay": "^3.30.2",
    "@pothos/plugin-scope-auth": "^3.16.0",
    "@prisma/client": "^4.8.0",
    "@sentry/integrations": "^7.28.1",
    "@sentry/node": "^7.28.1",
    "apollo-server-express": "^3.11.1",
    "bandcamp-scraper": "^1.5.0",
    "cookie-parser": "^1.4.6",
    "crc-32": "^1.2.2",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.4",
    "dotenv": "^16.0.3",
    "dotenv-vault": "^1.17.0",
    "email-validator": "^2.0.4",
    "env-var": "^7.3.0",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "googleapis": "^110.0.0",
    "graphile-worker": "^0.13.0",
    "graphql": "^16.5.0",
    "graphql-scalars": "^1.20.1",
    "jsonwebtoken": "^8.5.1",
    "lodash.merge": "^4.6.2",
    "node-emoji": "^1.11.0",
    "node-fetch": "2",
    "nodemailer": "^6.8.0",
    "normalize-url": "6",
    "samlify": "^2.8.7",
    "ts-node": "^10.8.1",
    "ts-proto": "^1.136.1",
    "yarn": "^1.22.19"
  }
}
