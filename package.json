{
  "name": "api.kulturspektakel.de",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "node -r dotenv/config dist/index.js",
    "build": "tsc",
    "dev": "ts-node-dev --transpile-only --no-notify -r dotenv/config src/index.ts",
    "generate:prisma": "prisma generate",
    "generate:mail": "maizzle build production",
    "generate:proto": "cd src/proto && protoc --experimental_allow_proto3_optional --plugin=../../node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. ./*.proto --ts_proto_opt=esModuleInterop=true",
    "generate:env": "yarn dotenv-vault pull",
    "update:db": "export $(cat .env | xargs) && prisma db push",
    "mail": "maizzle serve",
    "db": "ssh -L 5432:localhost:5432 kultursp@kulturspektakel.de"
  },
  "devDependencies": {
    "@maizzle/framework": "4.6.1",
    "@sentry/cli": "^2.21.3",
    "@types/basic-auth": "^1.1.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/date-fns": "^2.6.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash.merge": "^4.6.9",
    "@types/node-fetch": "2.6.9",
    "@types/nodemailer": "^6.4.14",
    "@types/totp-generator": "^0.0.6",
    "prettier": "^3.0.3",
    "prisma": "^5.5.2",
    "tailwindcss-box-shadow": "^2.0.1",
    "tailwindcss-email-variants": "^2.0.3",
    "tailwindcss-mso": "^1.3.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@apollo/server-plugin-landing-page-graphql-playground": "^4.0.1",
    "@awaitjs/express": "^0.9.0",
    "@aws-sdk/client-ses": "^3.449.0",
    "@google-cloud/logging": "^11.0.0",
    "@pothos/core": "^3.38.0",
    "@pothos/plugin-prisma": "^3.61.0",
    "@pothos/plugin-relay": "^3.44.0",
    "@pothos/plugin-scope-auth": "^3.20.0",
    "@prisma/client": "^5.5.2",
    "@sentry/integrations": "^7.80.0",
