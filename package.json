    "googleapis": "^120.0.0",
    "graphile-worker": "^0.13.0",
    "graphql": "^16.7.1",
    "graphql-scalars": "^1.22.2",
    "jsonwebtoken": "^9.0.0",
    "lodash.merge": "^4.6.2",
    "node-fetch": "2",
    "nodemailer": "^6.9.3",
    "normalize-url": "6",
    "samlify": "^2.8.10",
    "slugify": "^1.6.6",
