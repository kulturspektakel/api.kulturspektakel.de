{
  "name": "api.kulturspektakel.de",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "node -r dotenv/config dist/index.js",
    "build": "tsc",
    "dev": "ts-node-dev --transpile-only --no-notify -r dotenv/config src/index.ts",
    "generate:prisma": "prisma generate",
    "generate:mail": "maizzle build production",
    "generate:proto": "cd src/proto && protoc --experimental_allow_proto3_optional --plugin=../../node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. ./*.proto --ts_proto_opt=esModuleInterop=true",
    "generate:env": "yarn dotenv-vault pull",
    "update:db": "export $(cat .env | xargs) && prisma db push",
    "mail": "maizzle serve"
  },
  "devDependencies": {
    "@maizzle/framework": "4.4.6",
    "@sentry/cli": "^2.19.4",
    "@types/basic-auth": "^1.1.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/date-fns": "^2.6.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash.merge": "^4.6.7",
    "@types/node-fetch": "2.6.4",
    "@types/nodemailer": "^6.4.8",
    "@types/totp-generator": "^0.0.5",
    "prettier": "^2.8.8",
    "prisma": "^4.16.1",
    "tailwindcss-box-shadow": "^2.0.1",
    "tailwindcss-email-variants": "^2.0.2",
    "tailwindcss-mso": "^1.3.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.5"
  },
  "dependencies": {
    "@apollo/server": "^4.7.5",
    "@apollo/server-plugin-landing-page-graphql-playground": "^4.0.1",
    "@awaitjs/express": "^0.9.0",
    "@aws-sdk/client-ses": "^3.362.0",
    "@google-cloud/logging": "^10.5.0",
    "@pothos/core": "^3.30.0",
    "@pothos/plugin-prisma": "^3.52.0",
    "@pothos/plugin-relay": "^3.43.0",
    "@pothos/plugin-scope-auth": "^3.19.2",
    "@prisma/client": "^4.16.1",
    "@sentry/integrations": "^7.57.0",
    "@sentry/node": "^7.57.0",
    "bandcamp-scraper": "^1.5.0",
    "basic-auth": "^2.0.1",
    "cookie-parser": "^1.4.6",
    "crc-32": "^1.2.2",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
