{
  "name": "api.kulturspektakel.de",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "node -r dotenv/config dist/index.js",
    "dev": "ts-node-dev --transpile-only --no-notify -r dotenv/config src/index.ts",
    "build": "yarn generate && tsc",
    "generate": "yarn generate:prisma && yarn generate:graphql",
    "generate:graphql": "graphql-codegen -r dotenv/config",
    "generate:prisma": "prisma generate",
    "update:db": "export $(cat .env | xargs) && prisma db push --preview-feature --accept-data-loss"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/introspection": "^1.17.8",
    "@graphql-codegen/typescript": "^1.19.0",
    "@graphql-codegen/typescript-graphql-request": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@types/cookie-parser": "^1.4.2",
    "@types/date-fns": "^2.6.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node-fetch": "^2.5.8",
    "prettier": "^2.0.5",
    "prisma": "^2.21.2",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@prisma/client": "^2.21.2",
    "@sendgrid/mail": "^7.4.2",
    "apollo-server-express": "^2.21.0",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.19.0",
    "dotenv": "^8.2.0",
    "email-regex": "^4.0.0",
    "env-var": "^6.3.0",
    "form-data": "^4.0.0",
    "graphql": "^15.4.0",
    "graphql-request": "^3.4.0",
    "graphql-scalars": "^1.3.0",
    "ics": "^2.27.0",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.32.0",
    "node-fetch": "^2.6.1",
    "passkit-generator": "^2.0.6"
  }
}
