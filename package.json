{
  "name": "api.kulturspektakel.de",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "node -r dotenv/config dist/index.js",
    "dev": "ts-node-dev --transpile-only --no-notify -r dotenv/config src/index.ts",
    "build": "yarn generate && tsc",
    "generate": "yarn generate:env && yarn generate:prisma && yarn generate:graphql && yarn generate:mail",
    "generate:graphql": "graphql-codegen -r dotenv/config",
    "generate:prisma": "prisma generate",
    "generate:mail": "maizzle build production",
    "generate:proto": "cd src/proto && protoc --experimental_allow_proto3_optional --plugin=../../node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. ./*.proto --ts_proto_opt=esModuleInterop=true",
    "generate:env": "yarn dotenv-cli pull",
    "update:db": "export $(cat .env | xargs) && prisma db push",
    "mail": "maizzle serve"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-graphql-request": "^4.3.7",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@maizzle/cli": "^1.4.0",
    "@maizzle/framework": "^3.7.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/date-fns": "^2.6.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash.merge": "^4.6.6",
    "@types/node-emoji": "^1.8.1",
    "@types/node-fetch": "2.6.1",
    "@types/nodemailer": "^6.4.1",
    "prettier": "^2.5.1",
    "prisma": "^3.9.2",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.52.0",
    "@dotenv/cli": "^2.0.0",
    "@google-cloud/error-reporting": "^2.0.4",
    "@prisma/client": "^3.9.2",
    "apollo-server-express": "^3.6.3",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.2.2",
    "dotenv": "^16.0.0",
    "email-validator": "^2.0.4",
    "env-var": "^7.1.1",
    "express": "^4.17.3",
    "form-data": "^4.0.0",
    "graphile-worker": "^0.12.2",
    "graphql": "^16.3.0",
    "graphql-scalars": "^1.14.1",
    "ics": "^2.35.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.merge": "^4.6.2",
    "nexus": "^1.2.0",
    "nexus-prisma": "^0.35.0",
    "node-emoji": "^1.11.0",
    "node-fetch": "2",
    "nodemailer": "^6.7.2",
    "normalize-url": "6",
    "passkit-generator": "^2.0.8",
    "ts-node": "^10.5.0",
    "ts-proto": "^1.105.2"
  }
}
